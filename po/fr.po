msgid ""
"Request an IP address from DHCP server\n"
"sudo dhclient\n"
msgstr ""
"Demande une adresse IP à un serveur DHCP\n"
"sudo dhclient\n"

#: ../ailurus/common/tips.py:170
msgid ""
"Temporarily restart an init script\n"
"sudo /etc/init.d/SCRIPT_NAME restart\n"
"Temporarily stop an init script\n"
"sudo /etc/init.d/SCRIPT_NAME stop\n"
msgstr ""
"Redémarrer temporairement un script init\n"
"sudo /etc/init.d/SCRIPT_NAME restart\n"
"Stopper temporairement un script init \n"
"sudo /etc/init.d/SCRIPT_NAME stop\n"

#: ../ailurus/common/tips.py:176
msgid ""
"Add a user\n"
"sudo adduser USER_NANE\n"
"Delete a user\n"
"sudo deluser USER_NAME\n"
msgstr ""
"Ajouter un utilisateur\n"
"sudo adduser NOM_UTILISATEUR\n"
"Effacer un utilisateur\n"
"sudo deluser NOM_UTILISATEUR\n"

#: ../ailurus/common/tips.py:182
msgid ""
"Change user password\n"
"sudo passwd USER_NAME\n"
msgstr ""
"Changer le mot de passe d'un utilisateur\n"
"sudo passwd NOM_UTILISATEUR\n"

