msgid ""
"List files which are opened by a given process\n"
"lsof -p PROCESS_ID\n"
"lsof -c PROCESS_NAME\n"
msgstr ""
"Liste les fichiers ouverts par un processus donné\n"
"lsof -p IDENTIFICATEUR_DU_PROCESSUS\n"
"lsof -c NOM_DU_PROCESSUS\n"

#: ../ailurus/common/tips.py:120
msgid ""
"List processes which opened a given file\n"
"lsof FILE_NAME\n"
msgstr ""
"Liste les processus avec le fichier ouvert donné\n"
"lsof NOM_FICHIER\n"

#: ../ailurus/common/tips.py:124
msgid ""
"List processes which are using port 80\n"
"lsof -i :80\n"
msgstr ""
"Liste les processus utilisant le port 80\n"
"lsof -i :80\n"

#: ../ailurus/common/tips.py:128
msgid ""
"Configure an ADSL connection\n"
"sudo pppoeconf\n"
msgstr ""
"Configurer une connexion ADSL\n"
"sudo pppoeconf\n"

