msgid ""
"Display NetBIOS name of a given IP address\n"
"nmblookup -A IP_ADDRESS\n"
msgstr ""
"Affiche le nom NetBIOS d'une adresse IP donnée\n"
"nmblookup -A ADRESSE_IP\n"

#: ../ailurus/common/tips.py:146
msgid ""
"Display IP address and MAC\n"
"ifconfig -a\n"
msgstr ""
"Affiche les adresses IP et MAC\n"
"ifconfig -a\n"

#: ../ailurus/common/tips.py:150
msgid ""
"Display route\n"
"netstat -rn\n"
msgstr ""
"Affiche les routes\n"
"netstat -rn\n"

