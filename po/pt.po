#: .././ailurus/common/tips.py:240
msgid ""
"Create two empty files\n"
"touch file_name_1 file_name_2\n"
msgstr ""
"Criar 2 ficheiros vazios\n"
"touch file_name_1 file_name_2\n"

#: .././ailurus/common/tips.py:244
msgid ""
"Create directory. Create parent directories as needed.\n"
"mkdir -p /tmp/a/b/c/d/e\n"
msgstr ""
"Criar directório. Criar directório superior se necessário.\n"
"mkdir -p /tmp/a/b/c/d/e\n"

#: .././ailurus/common/tips.py:248
msgid ""
"Change working directory to the home folder\n"
"cd\n"
"Change working directory to the previous working directory\n"
"cd -\n"
msgstr ""
"Alterar directório de trabalho para a pasta pessoal\n"
"cd\n"
"Alterar directório de trabalho para o directório de trabalho anterior\n"
"cd-\n"

#: .././ailurus/common/tips.py:254
msgid ""
"Display hidden files\n"
"ls -a\n"
msgstr ""
"Mostar ficheiros ocultos\n"
"ls -a\n"

#: .././ailurus/common/tips.py:258
msgid ""
"Copy directory. Preserve links, file mode, ownership, timestamps. \n"
"cp -a SOURCE_DIRECTORY DEST_DIRECTORY\n"
msgstr ""
"Copiar directório. Preservar ligações, modo, titularidade e estampas. \n"
"cp -a SOURCE_DIRECTORY DEST_DIRECTORY\n"

#: .././ailurus/common/tips.py:262
msgid ""
"Determine file type\n"
"file FILE_NAME\n"
msgstr ""
"Determinar tipo de ficheiro\n"
"file FILE_NAME\n"

#: .././ailurus/common/tips.py:266
msgid ""
"Output the last 6 lines\n"
"tail -n 6 FILE_NAME\n"
msgstr ""
"Resultado das últimas 6 linhas\n"
"tail -n 6 FILE_NAME\n"

#: .././ailurus/common/tips.py:270
msgid ""
"Copy files via SSH\n"
"scp -rp FILE_NAME USERNAME@HOST:DEST_PATH\n"
msgstr ""
"Copiar ficheiros via SSH\n"
"scp -rp FILE_NAME USERNAME@HOST:DEST_PATH\n"

#: .././ailurus/common/tips.py:274
msgid ""
"Rename '*.rm' files to '*.rmvb' files\n"
"rename 's/.rm$/.rmvb/' *\n"
msgstr ""
"Renomear ficheiros '*.rm' para '*.rmvb'\n"
"rename 's/.rm$/.rmvb/' *\n"

#: .././ailurus/common/tips.py:278
msgid ""
"Change the file name to lowercase\n"
"rename 'tr/A-Z/a-z/' *\n"
msgstr ""
"Alterar o nome no ficheiro para minúsculas\n"
"rename 'tr/A-Z/a-z/' *\n"

#: .././ailurus/common/tips.py:282
msgid ""
"Display subdirectories in current directory\n"
"ls -d */.\n"
msgstr ""
"Mostar sub-directórios no directório actual\n"
"ls -d */.\n"

#: .././ailurus/common/tips.py:286
msgid ""
"Display file number in current directory\n"
"ls . | wc -w\n"
msgstr ""
"Mostar número do ficheiro no directório actual\n"
"ls . | wc -w\n"

#: .././ailurus/common/tips.py:290
msgid ""
"Extract \"*.gz\" file\n"
"gunzip FILE_NAME.gz\n"
"Extract \"*.tar.gz\" file\n"
"tar zxf FILE_NAME.tar.gz\n"
"Extract \"*.tar.bz2\" file\n"
"tar jxf FILE_NAME.tar.bz2\n"
msgstr ""
"Extrair ficheiro \"*.gz\"\n"
"gunzip FILE_NAME.gz\n"
"Extrair ficheiro\"*.tar.gz\"\n"
"tar zxf FILE_NAME.tar.gz\n"
"Extrair ficheiro\"*.tar.bz2\"\n"
"tar jxf FILE_NAME.tar.bz2\n"

#: .././ailurus/common/tips.py:298
msgid ""
"Do compression\n"
"tar czf FILE_NAME.tar.gz FILE1 FILE2 FILE3\n"
"tar cjf FILE_NAME.tar.bz2 FILE1 FILE2 FILE3\n"
msgstr ""
"Comprimir\n"
"tar czf FILE_NAME.tar.gz FILE1 FILE2 FILE3\n"
"tar cjf FILE_NAME.tar.bz2 FILE1 FILE2 FILE3\n"

#: .././ailurus/common/tips.py:303
msgid ""
"Displays a calendar\n"
"cal\n"
"cal MONTH YEAR\n"
msgstr ""
"Mostar como calendário\n"
"cal\n"
"cal MONTH YEAR\n"

#: .././ailurus/common/tips.py:308
msgid ""
"Set the date and time via NTP\n"
"sudo ntpdate ntp.ubuntu.com\n"
msgstr ""
"Definir data e hora via NTP\n"
"sudo ntpdate ntp.ubuntu.com\n"

#: .././ailurus/common/tips.py:312
msgid ""
"Poweroff your computer\n"
"sudo halt\n"
"sudo shutdown -h now\n"
"Poweroff your computer in 23:00\n"
"sudo shutdown -h 23:00\n"
"Poweroff your computer after 60 minutes\n"
"sudo shutdown -h +60\n"
msgstr ""
"Desligar o computador\n"
"sudo halt\n"
"sudo shutdown -h now\n"
"Desligar o computador às 23:00\n"
"sudo shutdown -h 23:00\n"
"Desligar o computador daqui a 60 minutos\n"
"sudo shutdown -h +60\n"

#: .././ailurus/common/tips.py:321
msgid ""
"Reboot your computer\n"
"sudo reboot\n"
"sudo shutdown -r now\n"
msgstr ""
"Reiniciar o computador\n"
"sudo reboot\n"
"sudo shutdown -r now\n"

#: .././ailurus/common/tips.py:326
msgid ""
"If you want some program to start up automatically, please put '.desktop' "
"files into '~/.config/autostart'\n"
msgstr ""
"Se quiser que algum programa inicie automaticamente, coloque os ficheiros "
"'.desktop' em '~/.config/autostart'\n"

#: .././ailurus/common/tips.py:329
msgid ""
"You can configure \"preferred applications\" by this file "
"\"~/.local/share/applications/mimeapps.list\"\n"
msgstr ""
"Pode configurar as \"aplicações preferidas\" neste ficheiro "
"\"~/.local/share/applications/mimeapps.list\"\n"

#: .././ailurus/common/tips.py:332
msgid ""
"Continuously monitor the memory usage\n"
"watch -d free\n"
msgstr ""
"Controlar continuamente a utilização da memória\n"
"watch -d free\n"

#: .././ailurus/common/tips.py:336
msgid ""
"Display HTTP HEAD response\n"
"w3m -dump_head http://example.com\n"
msgstr ""
"Mostar resposta de HTTP HEAD\n"
"w3m -dump_head http://example.com\n"

#: .././ailurus/common/tips.py:340
msgid ""
"Display file content with line number\n"
"nl FILE_NAME\n"
msgstr ""
"Mostar o conteúdo do ficheiro com n.º de linha\n"
"nl FILE_NAME\n"

#: .././ailurus/common/tips.py:344
msgid ""
"Eliminate Rootkit\n"
"sudo rkhunter --checkall\n"
msgstr ""
"Eliminar Rootkit\n"
"sudo rkhunter --checkall\n"

#: .././ailurus/common/tips.py:348
msgid ""
"Change hostname\n"
"sudo hostname new_name\n"
msgstr ""
"Alterar nome de anfitrião\n"
"sudo hostname new_name\n"

#: .././ailurus/common/tips.py:352
msgid ""
"\"Tasksel\" group software packages into \"task\"s. You can select a "
"\"task\" and then install all necessary software packages. It is easy to set "
"up LAMP servers or cloud computing servers.\n"
"Show all tasks\n"
"tasksel --list\n"
"Display the extended description of a task\n"
"tasksel --task-desc lamp-server\n"
"List the packages which are parts of a task\n"
"tasksel --task-packages lamp-server\n"
"Install/remove a task\n"
"gksudo tasksel\n"
msgstr ""
"O \"Tasksel\" agrupa os pacotes em \"Tarefas\". Você pode seleccionar uma "
"\"tarefa\" e instalar todos os pacotes necessários. Ideal para configurar "
"servidores LAMP ou servidores cloud computing.\n"
"Mostrar todas as tarefas\n"
"tasksel --list\n"
"Mostrar os detalhes das tarefas\n"
"tasksel --task-desc lamp-server\n"
"Listar os pacotes que pertencem à tarefa\n"
"tasksel --task-packages lamp-server\n"
"Instalar/remover uma tarefa\n"
"gksudo tasksel\n"

