msgid ""
"Add a user\n"
"sudo adduser USER_NANE\n"
"Delete a user\n"
"sudo deluser USER_NAME\n"
msgstr ""
"Añadir un usuario\n"
"sudo adduser NOMBRE_DE_USUARIO\n"
"Eliminar un usuario\n"
"sudo deluser NOMBRE_DE_USUARIO\n"

